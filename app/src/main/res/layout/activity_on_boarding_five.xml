<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color1"
    tools:context=".OnBoardingFiveActivity">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:text="Apa aktivitas sehari-harimu?"
        android:textColor="@color/color3"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_kecil" />

    <TextView
        android:id="@+id/tv_logo8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="FitMe"
        android:textColor="@color/color3"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_top_corner" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_next_onboarding2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/shape_rectangle_color4"
        android:text="Hitung kebutuhan kalorimu"
        app:backgroundTint="@color/color4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/cb_mendaki2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/poppins"
        android:text="Mendaki"
        android:textColor="@color/color2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cb_aerobik2" />

    <CheckBox
        android:id="@+id/cb_membaca2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:checked="false"
        android:fontFamily="@font/poppins"
        android:text="Membaca"
        android:textColor="@color/color2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <CheckBox
        android:id="@+id/cb_menyapu2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins"
        android:text="Menyapu"
        android:textColor="@color/color2"
        app:layout_constraintEnd_toStartOf="@+id/cb_aerobik2"
        app:layout_constraintStart_toEndOf="@+id/cb_membaca2"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <CheckBox
        android:id="@+id/cb_aerobik2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppins"
        android:text="Aerobik"
        android:textColor="@color/color2"
        app:layout_constraintStart_toStartOf="@+id/cb_mendaki2"
        app:layout_constraintTop_toTopOf="@+id/imageView9" />

    <CheckBox
        android:id="@+id/cb_menyetir2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Menyetir"
        android:textColor="@color/color2"
        app:layout_constraintStart_toStartOf="@+id/cb_membaca2"
        app:layout_constraintTop_toBottomOf="@+id/cb_membaca2" />

    <CheckBox
        android:id="@+id/cb_jalan_cepat2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Jalan Cepat"
        android:textColor="@color/color2"
        app:layout_constraintStart_toEndOf="@+id/cb_membaca2"
        app:layout_constraintStart_toStartOf="@+id/cb_menyapu2"
        app:layout_constraintTop_toBottomOf="@+id/cb_menyapu2" />

    <CheckBox
        android:id="@+id/cb_berjalan2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Berjalan"
        android:textColor="@color/color2"
        app:layout_constraintStart_toStartOf="@+id/cb_menyetir2"
        app:layout_constraintTop_toBottomOf="@+id/cb_menyetir2" />

    <CheckBox
        android:id="@+id/cb_bersepeda2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Bersepeda"
        android:textColor="@color/color2"
        app:layout_constraintStart_toStartOf="@+id/cb_jalan_cepat2"
        app:layout_constraintTop_toBottomOf="@+id/cb_jalan_cepat2" />

    <CheckBox
        android:id="@+id/cb_jogging2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins"
        android:text="Jogging"
        android:textColor="@color/color2"
        app:layout_constraintStart_toStartOf="@+id/cb_mendaki2"
        app:layout_constraintTop_toBottomOf="@+id/cb_mendaki2" />

</androidx.constraintlayout.widget.ConstraintLayout>